{
    "name": "vasilyevmichail-spec/configs-server-client-laravel",
    "description": "Laravel package for loading configuration from custom .my_env file with early loading support",
    "type": "library",
    "version": "1.0.3",
    "keywords": ["laravel", "config", "env", "configuration", "dotenv"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0",
        "illuminate/config": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "VM\\ConfigManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VM\\ConfigManager\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "VM\\ConfigManager\\ConfigManagerServiceProvider"
            ],
            "aliases": {
                "ConfigManager": "VM\\ConfigManager\\Facades\\ConfigManager"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}