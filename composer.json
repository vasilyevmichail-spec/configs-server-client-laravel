{
    "name": "vasilyevmichail-spec/configs-server-client-laravel",
    "description": "Laravel configuration manager with .env priority support",
    "type": "library",
    "license": "MIT",
    "version": "1.0.0",
    "autoload": {
        "psr-4": {
            "VM\\ConfigManager\\": "src/"
        }
    },
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0",
        "illuminate/config": "^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "VM\\ConfigManager\\ConfigServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}